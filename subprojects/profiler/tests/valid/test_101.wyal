define Map<K,V> ({K,V} pairs) is:
  forall((K k1, V v1) in pairs, (K k2, V v2) in pairs):
    if:
      k1 == k2
    then:
      v1 == v2 

define List({(int,int)} pairs) is:
  Map<int,int>(pairs)
  forall (int i, int v) in pairs:
      i â‰¥ 0 &&
      i < |pairs|

assert forall({int,int} xs) ; List(xs) ==> Map<int,int>(xs)


